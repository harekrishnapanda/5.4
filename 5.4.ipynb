    "# 5.4\n",
    "def movavg(lst,k):\n",
    "    x=len(lst)-k+1\n",
    "    y=[]\n",
    "    for i in range(x):\n",
    "        sum=0\n",
    "        for j in range(k):\n",
    "            sum +=lst[i+j]\n",
    "        z=sum/k\n",
    "        y.append(int(z))\n",
    "    print(y)\n",
    "    \n",
    "lst1=[3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150,200]\n",
    "movavg(lst1,3)"
   ]
